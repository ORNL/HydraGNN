{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": "PowerGrid Preonly Pickle",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/examples/power_grid/power_grid.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/examples/power_grid",
            "args": ["--preonly", "--pickle"]
        },
        {
            "name": "PowerGrid Pickle",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/examples/power_grid/power_grid.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/examples/power_grid",
            "args": ["--pickle"]
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}"
        }
    ]
}