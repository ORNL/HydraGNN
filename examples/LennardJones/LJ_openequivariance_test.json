{
  "Verbosity": {
    "level": 2
  },
  "Dataset": {
    "name": "LJdataset",
    "format": "XYZ",
    "node_features": {
      "name": ["atom_type"],
      "dim": [1],
      "column_index": [0]
    },
    "graph_features":{
      "name": ["total_energy"],
      "dim": [1],
      "column_index": [0]
    }
  },
  "NeuralNetwork": {
    "Architecture": {
      "periodic_boundary_conditions": true,
      "mpnn_type": "MACE",
      "radius": 5.0,
      "max_neighbours": 5,
      "int_emb_size": 32,
      "out_emb_size": 16,
      "basis_emb_size": 8,
      "num_gaussians": 10,
      "num_filters": 8,
      "num_before_skip": 1,
      "num_after_skip": 1,
      "num_radial": 16,
      "radial_type": "bessel",
      "distance_transform": "None", 
      "envelope_exponent": 5,
      "hidden_dim": 64,
      "num_conv_layers": 2,
      "task_weights": [1.0],
      "max_ell": 2,
      "node_max_ell": 1,
      "correlation": 3,
      "avg_num_neighbors": 4.0,
      "enable_openequivariance": true,
      "output_heads": {
        "graph": {
          "num_sharedlayers": 0,
          "dim_sharedlayers": 0,
          "num_headlayers": 2,
          "dim_headlayers": [64, 32],
          "type": "graph"
        }
      }
    },
    "Training": {
      "num_epoch": 5,
      "batch_size": 32,
      "Checkpoint": true,
      "checkpoint_warmstart": "restart",
      "Optimizer": {
        "type": "AdamW",
        "learning_rate": 0.001
      },
      "conv_checkpointing": false
    },
    "Variables_of_interest": {
      "input_node_features": [0],
      "output_names": ["total_energy"],
      "type": ["graph"],
      "size": [1],
      "output_index": [0]
    }
  },
  "Visualization": {
    "plot_init_solution": false,
    "plot_hist_solution": false,
    "create_plots": false
  }
}