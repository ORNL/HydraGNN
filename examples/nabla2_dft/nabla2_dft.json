{
  "Verbosity": {
    "level": 2
  },
  "NeuralNetwork": {
    "Architecture": {
      "mpnn_type": "EGNN",
      "edge_features": ["bond_length"],
      "equivariance": true,
      "max_neighbours": 40,
      "num_gaussians": 50,
      "num_filters": 64,
      "envelope_exponent": 5,
      "int_emb_size": 64,
      "basis_emb_size": 8,
      "out_emb_size": 128,
      "num_after_skip": 2,
      "num_before_skip": 1,
      "num_radial": 6,
      "num_spherical": 7,
      "radius": 5.0,
      "hidden_dim": 200,
      "num_conv_layers": 6,
      "output_heads": {
        "graph": {
          "num_sharedlayers": 2,
          "dim_sharedlayers": 200,
          "num_headlayers": 2,
          "dim_headlayers": [512, 256]
        },
        "node": {
          "num_headlayers": 2,
          "dim_headlayers": [512, 256],
          "type": "mlp"
        }
      },
      "task_weights": [1.0, 50.0]
    },
    "Variables_of_interest": {
      "input_node_features": [0],
      "output_index": [0, 1],
      "type": ["graph", "node"],
      "output_dim": [1, 3],
      "output_names": ["formation_energy", "forces"],
      "denormalize_output": false
    },
    "Training": {
      "Checkpoint": true,
      "num_epoch": 50,
      "batch_size": 32,
      "continue": 0,
      "startfrom": "",
      "Optimizer": {
        "learning_rate": 0.001
      }
    }
  }
}
