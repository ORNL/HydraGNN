{
    "Dataset": {
        "name": "unit_test",
        "format": "unit_test",
        "path": "dataset/unit_test/output_files",
        "num_nodes": 8,
        "node_features": {
            "name": ["x","x2","x3"],
            "dim": [1, 1, 1],
            "column_index": [0, 6, 7]
        },
        "graph_features":{
            "name": [ "sum_x_x2_x3"],
            "dim": [1],
            "column_index": [0]
        }
    },
    "NeuralNetwork": {
        "Architecture": {
            "model_type": "PNN",
            "radius": 4,
            "max_neighbours": 4,
            "hidden_dim": 4,
            "num_conv_layers": 4,
            "output_heads": {
                "graph":{
                    "num_sharedlayers": 2,
                    "dim_sharedlayers": 4,
                    "num_headlayers": 2,
                    "dim_headlayers": [10,10]
                },
                "node": {
                    "num_headlayers": 2,
                    "dim_headlayers": [10,10]
                }
            },
            "task_weights": [1.0, 8.0, 8.0, 8.0]
        },
        "Variables_of_interest": {
            "input_node_features": [0],
            "output_names": ["sum_x_x2_x3","x","x2","x3"],
            "output_index": [0,0,1,2],
            "type": ["graph","node","node","node"],
            "denormalize_output": "False"
        },
        "Training": {
            "num_epoch": 200,
            "perc_train": 0.7,
            "learning_rate": 0.01,
            "batch_size": 32
        }
    }
}
