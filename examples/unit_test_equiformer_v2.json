{
    "Verbosity": {
        "level": 2
    },
    "NeuralNetwork": {
        "Profile": {"enable": 1},
        "Architecture": {
            "global_attn_engine": "EquiformerV2",
            "global_attn_heads": 8,
            "mpnn_type": "SchNet",
            "radius": 7,
            "max_neighbours": 5,
            "periodic_boundary_conditions": false,
            "pe_dim": 6,
            "equiformer_lmax_list": [6],
            "equiformer_mmax_list": [2],
            "equiformer_sphere_channels": 128,
            "edge_features": ["length"],
            "hidden_dim": 64,
            "num_conv_layers": 2,
            "int_emb_size": 32,
            "out_emb_size": 16,
            "basis_emb_size": 8,
            "num_gaussians": 10,
            "num_filters": 8,
            "num_before_skip": 1,
            "num_after_skip": 1,
            "envelope_exponent": 5,
            "max_ell": 1,
            "node_max_ell": 1,
            "num_radial": 5,
            "num_spherical": 2,
            "output_heads": {
                "graph":{
                    "num_sharedlayers": 2,
                    "dim_sharedlayers": 5,
                    "num_headlayers": 2,
                    "dim_headlayers": [50,25]
                }
            },
            "task_weights": [1.0]
        },
        "Variables_of_interest": {
            "input_node_features": [0],
            "output_names": ["free_energy"],
            "output_index": [0],
            "output_dim": [1],
            "type": ["graph"],
            "denormalize_output": false
        },
        "Training": {
            "num_epoch": 20,
            "batch_size": 32,
            "perc_train": 0.9,
            "patience": 1000,
            "optimizer": "Adam",
            "learning_rate": 0.02,
            "scheduler": "LinearLR",
            "loss_function_type": "mse",
            "conv_checkpointing": false
        },
        "Dataset": {
            "path": "./dataset/unit_test_singlehead_train/",
            "name": "unit_test_singlehead_train",
            "format": "XYZ",
            "node_features": {
                "name": ["atomic_number"]
            },
            "variables_log": false
        }
    }
}